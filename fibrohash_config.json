{
  "charset": {
    "allow_custom_charset": false,
    "extended_charset": "0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz!@#$%^&*()_+-=<>?{}[]|:;,.~`",
    "min_charset_size": 64
  },
  "cryptography": {
    "entropy_bit_length": 1024,
    "fibonacci_bit_length": 2048,
    "generation_rounds": {
      "high": 5,
      "maximum": 10,
      "standard": 3
    },
    "key_sizes": {
      "high": 64,
      "maximum": 128,
      "standard": 32
    },
    "pbkdf2_iterations": {
      "high": 5000,
      "maximum": 10000,
      "standard": 1000
    }
  },
  "logging": {
    "enable_security_logging": true,
    "log_failed_attempts": true,
    "log_file": "fibrohash.log",
    "log_level": "INFO"
  },
  "performance": {
    "generation_timeout_seconds": 30,
    "max_concurrent_generations": 10,
    "memory_limit_mb": 100
  },
  "security": {
    "allowed_security_levels": [
      "standard",
      "high",
      "maximum"
    ],
    "default_password_length": 32,
    "default_security_level": "high",
    "enforce_character_diversity": true,
    "max_input_length": 1000,
    "max_password_length": 128,
    "min_diversity_types": 3,
    "min_input_length": 1,
    "min_password_length": 8
  }
}